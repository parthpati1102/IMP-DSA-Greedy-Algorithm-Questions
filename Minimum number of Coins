class Solution {
  public:
    vector<int> minPartition(int N) {
        // code here
        int arr[] = {1 , 2 , 5 , 10 , 20 , 50 , 100 , 200 , 500 , 2000};
        
        vector<int> ans;
        
        for(int i = 9 ; i >= 0 ; i--){
            while(N >= arr[i]){
                N -= arr[i];
                ans.push_back(arr[i]);
            }
        }
        
        return ans;
    }
};
